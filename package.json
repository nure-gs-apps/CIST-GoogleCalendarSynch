{
  "name": "cist-google-calendar-sync",
  "version": "1.0.0",
  "description": "A project for synchronizating CIST with Google Calendar",
  "main": "dist/index.js",
  "engines": {
    "node": "^10.15.3"
  },
  "scripts": {
    "test": "jest",
    "i:g:dev": "sudo npm i rimraf tslint nodemon tslint-config-airbnb -g -D",
    "r:g:dev": "sudo npm r rimraf tslint nodemon tslint-config-airbnb -g -D",
    "i:g:dev:win": "npm i rimraf tslint nodemon tslint-config-airbnb -g -D",
    "r:g:dev:win": "npm r rimraf tslint nodemon tslint-config-airbnb -g -D",
    "build": "rimraf ./dist && ttsc",
    "start": "node ./dist",
    "start:b": "npm run build && npm start",
    "start:brk": "node --inspect-brk ./dist",
    "start:b:brk": "npm run build && npm run start:brk",
    "start:dev": "node --inspect=9229 ./dist",
    "start:b:dev": "npm run build && npm run start:dev",
    "watch": "nodemon -w src/ -w config/ -e ts,yaml -x \"npm run start:b:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nure-gs-apps/CIST-GoogleCalendarSynch.git"
  },
  "author": "Bogdan Shevchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nure-gs-apps/CIST-GoogleCalendarSynch/issues"
  },
  "homepage": "https://github.com/nure-gs-apps/CIST-GoogleCalendarSynch#readme",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "bluebird-global": "^1.0.1",
    "bottleneck": "^2.19.0",
    "config": "^3.1.0",
    "googleapis": "^39.2.0",
    "iconv": "^2.3.4",
    "inversify": "^5.0.1",
    "iterare": "^1.1.2",
    "js-yaml": "^3.13.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.9.3",
    "voca": "^1.4.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.27",
    "@types/bluebird-global": "^3.5.12",
    "@types/config": "0.0.34",
    "@types/iconv": "^2.1.16",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.0.4",
    "@types/voca": "^1.4.0",
    "@types/yargs": "^13.0.0",
    "ts-optchain": "^0.1.7",
    "ttypescript": "^1.5.6",
    "typescript": "^3.5.1"
  }
}
