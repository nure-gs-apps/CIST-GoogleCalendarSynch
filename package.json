    "@types/voca": "^1.4.0",
    "@types/yaml": "^1.2.0",
    "@types/yargs": "^15.0.3",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-nameof": "^4.2.2",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3"
  }
