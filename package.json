{
  "name": "cist-google-calendar-sync",
  "version": "1.0.0",
  "description": "A project for synchronizing NURE CIST with Google Calendar",
  "main": "dist/index.js",
  "engines": {
    "node": "^10.15.3"
  },
  "scripts": {
    "test": "jest",
    "lint": "tslint -p .",
    "start": "node ./dist",
    "clear": "rimraf ./dist",
    "build": "npm run clear && ttsc",
    "start:b": "npm run build && npm start",
    "start:brk": "node --inspect-brk ./dist",
    "start:b:brk": "npm run build && npm run start:brk",
    "start:dev": "node --inspect=9229 ./dist",
    "start:b:dev": "npm run build && npm run start:dev",
    "watch": "nodemon -w src/ -w config/ -e ts,yaml -x \"npm run start:b:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nure-gs-apps/CIST-GoogleCalendarSynch.git"
  },
  "author": "Bogdan Shevchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nure-gs-apps/CIST-GoogleCalendarSynch/issues"
  },
  "homepage": "https://github.com/nure-gs-apps/CIST-GoogleCalendarSynch#readme",
  "dependencies": {
    "@iarna/toml": "^2.2.3",
    "@types/moment-timezone": "^0.5.12",
    "app-root-path": "^3.0.0",
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "bluebird-global": "^1.0.1",
    "bottleneck": "^2.19.5",
    "change-case": "^4.1.1",
    "core-js": "^3.6.4",
    "googleapis": "^47.0.0",
    "iconv-lite": "^0.5.1",
    "inversify": "^5.0.1",
    "iterare": "^1.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "nconf": "^0.10.0",
    "proper-lockfile": "^4.1.1",
    "readonly-date": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.10.0",
    "voca": "^1.4.0",
    "yaml": "^1.7.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.29",
    "@types/bluebird-global": "^3.5.12",
    "@types/core-js": "^2.5.2",
    "@types/graceful-fs": "^4.1.3",
    "@types/lodash": "^4.14.149",
    "@types/nconf": "^0.10.0",
    "@types/node": "^12.12.37",
    "@types/proper-lockfile": "^4.1.1",
